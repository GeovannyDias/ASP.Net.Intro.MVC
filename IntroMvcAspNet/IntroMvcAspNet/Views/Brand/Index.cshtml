@*@{
    Layout="_CustomLayout"
    Layout = null;
}*@


@*@model List<IntroMvcAspNet.Models.Brand>;*@
@model List<Brand>;

<h1>Brands</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                Name
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var brand in Model)
        {
            <tr>
                <td>
                    @*@brand.Name*@
                    @Html.DisplayFor(m=>brand.Name)
                </td>
            </tr>  
        }
    </tbody>
</table>